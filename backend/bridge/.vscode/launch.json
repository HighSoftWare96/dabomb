  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "(BRIDGE) Launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceRoot}",
      "program": "${workspaceFolder}/node_modules/.bin/moleculer-runner",
      "args": [
        "services/"
      ],
      "env": {
        "NODE_ENV": "dev"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "SocketIO Client",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "dev"
      },
      "program": "${workspaceFolder}/bridge/tests/socketioClient.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}